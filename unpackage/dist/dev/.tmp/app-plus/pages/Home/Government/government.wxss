
.goverment-image{
	width:220rpx;
	height:196rpx;
	border-radius:10rpx;
	margin-right: 10rpx;
}
..goverment-image:nth-child(3){
	margin-right: 0;
}
.goverment-all{
	position: relative;
}
.goverment-all-image{
	position: absolute;
	top:0rpx;
	height: 50rpx;
	width: 120rpx;
}
.goverment-all-button{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	width:128rpx;
	height:48rpx;
	background:rgba(208,57,58,1);
	border-radius:24rpx;
	color: #FFFFFF;
	font-size: 30rpx;
}

