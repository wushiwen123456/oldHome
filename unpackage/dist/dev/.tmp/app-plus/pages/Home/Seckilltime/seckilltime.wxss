::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
}
.tab-view::before {
	content: '';
	position: absolute;
	border-bottom: 1rpx solid #eaeef1;
	-webkit-transform: scaleY(0.5);
	transform: scaleY(0.5);
	bottom: 0;
	right: 0;
	left: 0;
}
.tab-view {
	width: 100%;
	height: 140rpx;
	overflow: hidden;
	box-sizing: border-box;
	left: 0;
	z-index: 99;
	background: #FFFFFF;
	white-space: nowrap;
}
.tab-bar-item {
	/* padding: 0 28upx; */
	height: 140rpx;
	min-width: 160rpx;
	line-height: 100rpx;
	/* margin: 0 28upx; */
	display: inline-block;
	text-align: center;
	box-sizing: border-box;
}
.tab-bar-title {
	height: 40rpx;
	line-height: 100rpx;
	font-size: 34rpx;
	color: #525253;
}
.active {
	background: #CD3233;
}
.active .tab-bar-title {
	color: #FFFFFF !important;
	font-size: 34rpx;
}
/*tabbar end*/
.scoll-y {
	height: 100%;
}
.list-view {
	margin-top: 100rpx;
	width: 100%;
	background: #fff;
	box-sizing: border-box;
	padding-bottom: env(safe-area-inset-bottom);
}
.list-cell {
	padding: 30rpx;
	box-sizing: border-box;
}
.cell-title-box {
	display: -webkit-box;
	display: -webkit-flex;
