<mescroll-uni class="vue-ref" vue-id="1" up="{{upOption}}" down="{{downOption}}" data-ref="mescroll" data-event-opts="{{[['^down',[['downCallback']]],['^up',[['upCallback']]]]}}" bind:down="__e" bind:up="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="content"><view class="header"><image class="bgimg h-1" src="../../../static/tixian/nologin2.png" mode="aspectFill"></image><view class="h-tips margin-bottom-sm">我的余额（元）</view><view class="h-main"><view class="h-price">{{myPrice}}</view><view data-event-opts="{{[['tap',[['goTixian',['$event']]]]]}}" class="h-btn" bindtap="__e">提现</view></view></view><view class="ban-list"><view class="ban-title">资金明细</view><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{listData.length}}"><view class="{{['ban-item',(index!=listData.length-1)?'border-b':'']}}"><view class="item-center"><text class="text-black text-lg text-bold">{{item.pm==0?'支出':'收入'}}</text><view class="text-data">{{item.add_time}}</view></view><view class="item-right"><view class="item-r-price">{{(item.pm==0?'+':'-')+item.number+"元"}}</view></view></view></block></block></view><block wx:if="{{!listData.length}}"><view class="userNodes">您还没有记录</view></block></view><x-loading class="vue-ref" vue-id="{{('2')+','+('1')}}" text="加载中.." mask="true" click="true" data-ref="loading" bind:__l="__l"></x-loading></mescroll-uni>