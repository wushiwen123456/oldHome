.mybooking-image image {
  width: 164rpx;
  height: 164rpx;
}
.text-hieed {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.mybooking-haming {
  width: 48rpx;
  height: 48rpx;
  border-radius: 50%;
  margin-left: -10rpx;
  border: 2px solid #FFFFFF;
}
.mybooking-haming:nth-child(1) {
  margin-left: 0rpx;
}
.mybooking-button {
  width: 180rpx;
  height: 56rpx;
  line-height: 56rpx;
  background: none;
  border: 1px solid #cd3233;
  border-radius: 28rpx;
  color: #CD3233;
  font-size: 28rpx;
  margin: 0;
}
.mybooking-button-colse {
  border: 1px solid #999999;
  color: #999999;
}
.s-status {
  color: #cd3233;
  width: 400rpx;
  text-align: right;
}
.s-content {
  box-sizing: border-box;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding-right: 15rpx;
}
.s-content .s-item1 {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.button-buttom {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 25rpx 15rpx;
  margin: 0;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.button-buttom .b-button {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 376rpx;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.button-buttom .status {
    color: #CD3233;
}

