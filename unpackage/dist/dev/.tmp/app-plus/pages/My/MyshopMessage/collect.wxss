
.recordList-all{
	padding: 24rpx;
	position: relative;
}
.recordList-all-popu{
	position: absolute;
	z-index: 999;
	right: 30rpx;
	width:180rpx;
	height:80rpx;
	background:rgba(255,255,255,1);
	box-shadow:0rpx 0rpx 30rpx 6rpx rgba(0, 0, 0, 0.09);
	border-radius:10rpx;
	color: 32rpx;
	line-height: 80rpx;
	text-align: center;
}
.recordList-image{
	width:228rpx;
	height:228rpx;
}
.recordList-main{
	color: #525253;
	font-size: 28rpx;
	margin-top: 20rpx;
	margin-bottom: 10rpx;
	overflow:hidden;
	text-overflow:ellipsis; 
	display:-webkit-box; 
	-webkit-box-orient:vertical; 
	-webkit-line-clamp:2;
}
.recordList-main-all{
	width: 60%;
	height: 228rpx;
}
.winSize-zehzaho{
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 99;
	background: rgba(255,255,255,0);
}
.recordList-main-centent{
	width:120rpx;
	height:44rpx;
	border-radius:10rpx;
	text-align: center;
	line-height: 44rpx;
	font-size: 24rpx;
}

