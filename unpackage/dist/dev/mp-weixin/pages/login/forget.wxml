<view class="content-logoin"><canvas class="text-width" style="{{'height:'+(windowHeight+'px')+';'}}" disable-scroll="true"><view class="login-image"><image src="../../static/logo.png"></image></view><view><view class="login-all"><view class="login-message"><image src="../../static/phone.png"></image><input class="input-textlength" placeholder="请输入手机号码" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view></view><view class="login-all"><view class="flex align-center justify-between"><view class="login-message"><image src="../../static/name.png"></image><input class="input-textlength" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/></view><view class="solid-left"><view data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" class="margin-left" bindtap="__e">{{codeTip}}</view></view></view></view><view class="login-all"><view class="login-message"><image src="../../static/password.png"></image><input class="input-textlength" password="true" placeholder="请重置密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view></view></view><w-button vue-id="1" text="注 册" rotate="{{isRotate}}" data-event-opts="{{[['tap',[['startLogin']]]]}}" bindtap="__e" bind:__l="__l"></w-button></canvas></view>