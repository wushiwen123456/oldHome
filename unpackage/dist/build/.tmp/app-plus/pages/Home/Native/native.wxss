
page,.content{
	height: 100vh;
	overflow: hidden;
}
.native-banner{
	height: 256rpx;
	width: 100%;
}
.native-image-all{
	padding:40rpx 0;
}
.native-image-all image{
	height: 92rpx;
	width: 92rpx;
}
.native-list-image image{
	height: 180rpx;
	width: 180rpx;
}
.native-address{
	width: 140rpx;
	height: 60rpx;
	background: #F8F8F8;
	color: #333;
}
.native-txt-red{
	width:86rpx;
	height:40rpx;
	background:rgba(205,50,51,1);
	border-radius:16rpx;
	font-size: 20rpx;
}
.text-hide{
	overflow:hidden;
	text-overflow:ellipsis;
	display:-webkit-box; 
	-webkit-box-orient:vertical;
	-webkit-line-clamp:2;
}
.navtive-center{
	width: auto;
	display:inline-block !important;
	display:inline;
	height: 40rpx;
	line-height: 40rpx;
	margin: 16rpx 0;
	padding: 0 10rpx;
	background: #F9F9F9;
	color: #999999;
	font-size: 22rpx;
}
.navtive-select{
	width: 140rpx;
	height: 60rpx;
	background: #CD3233;
	color: #FFF;
}
.popModel{
	position: fixed;
	top: 42px;
}

