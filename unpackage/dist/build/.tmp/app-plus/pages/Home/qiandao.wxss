page, .content {
  height: 100vh;
  overflow: hidden;
}
.sing-all-text {
  background: #E8211F;
}
.sing-all-text image {
  width: 100%;
  height: 560rpx;
}
.sing-all {
  /* position: absolute;
	bottom: 0upx; */
  padding-bottom: 600rpx;
  width: 100%;
  /* height: 700upx; */
  background: #E8211F;
}
.sing-text {
  position: absolute;
  top: 280rpx;
  font-size: 60rpx;
  font-weight: bold;
  width: 100%;
  text-align: center;
  color: white;
  text-shadow: 0rpx 6rpx 5rpx #ff7a00;
}
.sing-data-imag {
  margin: 0 20rpx;
  width: 74rpx;
  height: 74rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
}
.sing-bottom-image {
  width: 26rpx;
  height: 26rpx;
}
.sing-time {
  width: 70rpx;
  height: 80rpx;
  margin-left: 20rpx;
  margin-bottom: 30rpx;
  border-radius: 12rpx;
}
.sing-noselect {
  background: #DCDCDC;
}
.sing-select {
  background: #DB3014;
  color: #FFFFFF;
}
.my-money {
  position: absolute;
  top: 40rpx;
  right: 0;
  width: 124rpx;
  height: 58rpx;
  background: #e45239;
  border-top-left-radius: 100rpx;
  border-bottom-left-radius: 100rpx;
}
.sing-all-main {
  border-radius: 24rpx;
}
.sing-animation {
  position: fixed;
  height: 100vh;
  width: 100vw;
  top: 0;
  z-index: 99999;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background-color: rgba(255, 255, 255, 0.7);
}
.sing-animation image {
    width: 84rpx;
    height: 80rpx;
    margin-right: 15rpx;
    -webkit-animation-timing-function: ease-out;
            animation-timing-function: ease-out;
    -webkit-animation-fill-mode: both;
            animation-fill-mode: both;
    -webkit-animation-duration: 1s;
            animation-duration: 1s;
    -webkit-animation-name: move;
            animation-name: move;
    display: inline-block;
}
.sing-animation .move-box {
    font-size: 60rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 100%;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    color: #f7ff71;
    font-weight: bolder;
    margin-top: -180rpx;
}
@-webkit-keyframes move {
0% {
    -webkit-transform: rotateY(0);
            transform: rotateY(0);
}
100% {
    -webkit-transform: rotateY(720deg);
            transform: rotateY(720deg);
}
}
@keyframes move {
0% {
    -webkit-transform: rotateY(0);
            transform: rotateY(0);
}
100% {
    -webkit-transform: rotateY(720deg);
            transform: rotateY(720deg);
}
}

