.enter-all-heigth {
  height: 100rpx;
}
.enter-left-image image {
  width: 62rpx;
  height: 62rpx;
}
.enter-border-bootom {
  border-top: #DCDCDC 1px solid;
}
.enter-border-bootom:nth-child(1) {
  border: none;
}
.entercode-button {
  margin-right: 24rpx;
  width: 220rpx;
  height: 60rpx;
  background: #cd3233;
  border-radius: 30rpx;
  font-size: 26rpx;
  color: #FFFFFF;
}
.enterNumber-all {
  border: #9A9A9A 1px dashed;
}
.enterNumber-all image {
  width: 320rpx;
  height: 200rpx;
}
.enter-button {
  margin: 60rpx 30rpx 30rpx 30rpx;
  background: #cd3233;
  border-radius: 50rpx;
  color: #FFFFFF;
  font-size: 32rpx;
}
page {
  height: 100%;
}
.page-1 {
  height: 100%;
}
.is-submit {
  width: 100%;
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #333;
  font-size: 32rpx;
}
.tips {
  color: #e54d42 !important;
}
.check-box-input {
  position: relative;
}
.input-icon {
  position: absolute;
  right: 15rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}

