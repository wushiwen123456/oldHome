.Myevaluate {
  margin-top: 20rpx;
  padding: 34rpx 24rpx 30rpx 24rpx;
  background: #FFFFFF;
}
.dis-flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.flex-item-cent {
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.flex-jus-space {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.Myevaluate-totle-head-img {
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  margin-right: 16rpx;
}
.Myevaluate-title-more {
  font-size: 60rpx;
  color: #999999;
}
.Myevaluate-title-type {
  color: #888888;
  font-size: 24rpx;
  margin: 20rpx 0 30rpx 0;
}
.Myevaluate-title-main {
  color: #333333;
  font-size: 30rpx;
  margin-bottom: 30rpx;
}
.Myevaluate-title-main-img {
  width: 226rpx;
  height: 226rpx;
  margin-right: 12rpx;
  margin-bottom: 4rpx;
}
.Myevaluate-title-main-img:nth-child(3n) {
  margin-right: 0rpx;
}
.Myevaluate-title-main-sanjiao {
  position: relative;
  width: 0;
  height: 0;
  border-width: 0 24rpx 24rpx;
  border-style: solid;
  border-color: transparent transparent #f5f5f5;
  margin-left: 80rpx;
}
.Myevaluate-title-main-huifu {
  padding: 24rpx 24rpx 24rpx 18rpx;
  background: #f5f5f5;
  border-radius: 10rpx;
  color: #666666;
}
.Myevaluate-title-classify {
  margin-top: 30rpx;
  background: #f5f5f5;
  color: #333333;
  padding: 10rpx;
}
.Myevaluate-title-classify image {
    margin-right: 20rpx;
}
.Myevaluate-title-classify image {
  width: 140rpx;
  height: 140rpx;
}
.Myevaluate-title-classify-name {
  max-width: 500rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.Myevaluate-title-classify-txt {
  margin-left: 40rpx;
  margin-top: 26rpx;
}
.Myevaluate-title-bottom {
  color: #999999;
  font-size: 24rpx;
  margin-top: 30rpx;
}
.Myevaluate-title-button {
  width: 140rpx;
  height: 42rpx;
  border-radius: 21rpx;
  border: solid 2rpx #fe4d3d;
  font-size: 24rpx;
  color: #fe4d3d;
  background: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  margin: 0;
}
.Myevaluate-title-txt {
  margin-left: 20rpx;
}
.Myevaluate-title-buttom-txt {
  margin-right: 36rpx;
}
.popups-txt-all {
  /* height: 260upx; */
  color: #333333;
  font-size: 32rpx;
  border-radius: 20rpx;
  width: 600rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}
.popups-txt {
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 100rpx;
}

