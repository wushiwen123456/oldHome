<view><block wx:for="{{MyevaluateList}}" wx:for-item="vo" wx:for-index="key" wx:key="key"><block wx:if="{{MyevaluateList.length}}"><view class="Myevaluate"><view class="dis-flex flex-jus-space flex-item-cent"><view class="dis-flex flex-item-cent"><image class="Myevaluate-totle-head-img" src="{{vo.avatar}}"></image><view>{{vo.nickname}}</view></view><view data-event-opts="{{[['tap',[['openPopupClick',['$event']]]]]}}" class="Myevaluate-title-more" bindtap="__e">···</view></view><view class="Myevaluate-title-type">{{vo.add_time+"  购买规格： "+(vo.suk||'默认')}}</view><view class="Myevaluate-title-main">{{vo.comment}}</view><block wx:if="{{MyevaluateList.merchant_reply_content}}"><view><view class="Myevaluate-title-main-sanjiao"></view><view class="Myevaluate-title-main-huifu"><label style="color:#333333;" class="_span">掌柜回复:</label>{{MyevaluateList.merchant_reply_content}}</view></view></block><block wx:if="{{vo.pics.length}}"><view class="Myevaluate-title-classify dis-flex"><block wx:for="{{vo.pics}}" wx:for-item="item" wx:for-index="index"><image src="{{item}}" data-event-opts="{{[['tap',[['lookDetail',['$0'],[[['MyevaluateList','',key],['pics','',index]]]]]]]}}" bindtap="__e"></image></block></view></block><view class="dis-flex flex-jus-space Myevaluate-title-bottom"><view class="dis-flex"><view>浏览0次</view><view class="Myevaluate-title-txt">点赞0次</view></view><view class="Myevaluate-title-buttom-txt Myevaluate-title-buttom-txt">已追评</view></view></view></block></block><uni-popup class="vue-ref" vue-id="1" type="center" data-ref="popups" bind:__l="__l" vue-slots="{{['default']}}"><view class="popups-txt-all"><view class="popups-txt" hover-class="havor-class" data-event-opts="{{[['tap',[['removeComment',['$event']]]]]}}" bindtap="__e">删除评论</view><view class="popups-txt" hover-class="havor-class" data-event-opts="{{[['tap',[['goodComment',['$event']]]]]}}" bindtap="__e">点赞</view><view class="popups-txt" hover-class="havor-class" data-event-opts="{{[['tap',[['closePopupsClick',['$event']]]]]}}" bindtap="__e">取消</view></view></uni-popup><x-loading class="vue-ref" vue-id="2" text="加载中..." mask="true" click="true" data-ref="loading" bind:__l="__l"></x-loading></view>